{
  "name": "andy-dale-profile",
  "version": "1.0.0",
  "description": "Andy Dale Profile project combining React and Node.js",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "node",
    "auth0",
    "leaflet"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/pr_tests/"
    ]
  },
  "author": "Andy Dale",
  "license": "ISC",
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@playwright/test": "^1.49.1",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.4.0",
    "sinricpro": "^2.12.1",
    "uuid": "^11.0.3",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "npm-force-resolutions": "^0.0.10",
    "supertest": "^7.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
